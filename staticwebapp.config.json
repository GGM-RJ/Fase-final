{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "*.css", "*.js", "/api/*"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}